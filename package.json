{
  "name": "terraformer",
  "displayName": "Terraformer",
  "description": "A VS Code custom visual editor for visualizing Terraform files",
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "terraformer.editor",
        "displayName": "Terraformer Visualizer",
        "selector": [
          {
            "filenamePattern": "*.tf"
          }
        ],
        "priority": "option"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "publisher": "fchieze",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Chieze-Franklin/terraformer.git"
  },
  "dependencies": {
    "@types/vscode": "^1.74.0",
    "express": "^4.18.2"
  },
  "keywords": []
}
